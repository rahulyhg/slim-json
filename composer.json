{
    "name": "slimphp-api/slim-json",
    "description": "A module for slim, JSON middleware for encoding based on Content-Type",
    "keywords": ["slim","json"],
    "require": {
        "php": ">5.6",
        "willdurand/negotiation": "^2.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "satooshi/php-coveralls": "^0.7.0@dev",
        "slim/slim": "3.*@dev"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Gabriel Baker",
            "email": "me@g403.co"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {"SlimApi\\Json\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {"SlimApi\\JsonTest\\": "tests/phpunit/"}
    }
}
